<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TicTacToe.javafx.controller.GameController">
   <children>
      <Label fx:id="player1" layoutX="14.0" layoutY="14.0" prefHeight="29.0" prefWidth="150.0" text="Player1">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="player2" alignment="CENTER_RIGHT" layoutX="436.0" layoutY="20.0" prefHeight="29.0" prefWidth="150.0" text="Player2">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <GridPane fx:id="gameGrid" layoutX="200.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
   </children>
</Pane>
